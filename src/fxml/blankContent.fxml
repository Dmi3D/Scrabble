<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="124.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.BlankContentController">
   <children>
      <Label layoutX="52.0" layoutY="13.0" prefHeight="59.0" prefWidth="336.0" text="PLEASE INDICATE WHAT LETTER '*' IS REPLACING IN THE WORD YOU PLACED:" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="letterReplaceTextField" alignment="CENTER" layoutX="146.0" layoutY="72.0" prefHeight="35.0" prefWidth="54.0" promptText="Letter" style="-fx-background-color: #203152; -fx-text-inner-color: white;" />
      <Button fx:id="replaceButton" layoutX="229.0" layoutY="72.0" mnemonicParsing="false" onAction="#handleButtonClicked" prefHeight="35.0" prefWidth="68.0" stylesheets="@../css/buttonStyle.css" text="REPLACE" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
