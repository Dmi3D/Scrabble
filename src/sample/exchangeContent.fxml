<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="124.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ExchangeContentController">
   <children>
      <Label layoutX="43.0" layoutY="6.0" prefHeight="56.0" prefWidth="353.0" text="PLEASE ENTER THE LETTERS YOU WISH TO EXCHANGE WITHOUT SPACES" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <TextField fx:id="lettersInputField" layoutX="73.0" layoutY="69.0" onKeyReleased="#handleKeyReleased" prefHeight="30.0" prefWidth="146.0" promptText="Letters" style="-fx-background-color: #203152;" />
      <Button fx:id="exchangeButton" alignment="CENTER" layoutX="250.0" layoutY="69.0" mnemonicParsing="false" onAction="#onButtonClicked" prefHeight="30.0" prefWidth="95.0" style="-fx-background-color: #203152;" text="EXCHANGE" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
