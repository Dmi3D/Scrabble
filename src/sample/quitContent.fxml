<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="124.0" prefWidth="442.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.QuitContentController">
   <children>
      <Label layoutX="57.0" layoutY="14.0" prefHeight="64.0" prefWidth="326.0" text="IF YOU QUIT, ALL PROGRESS WILL BE LOST. DO YOU WISH TO CONTINUE?" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Button fx:id="yesButton" layoutX="173.0" layoutY="85.0" mnemonicParsing="false" onAction="#onButtonClicked" style="-fx-background-color: #203152;" text="YES" textFill="WHITE" AnchorPane.leftAnchor="170.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="noButton" layoutX="228.0" layoutY="85.0" mnemonicParsing="false" onAction="#onButtonClicked" style="-fx-background-color: #203152;" text="NO" textFill="WHITE" AnchorPane.rightAnchor="170.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
